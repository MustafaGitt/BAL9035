<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target xsi:type="File" name="jsonFile" fileName="${basedir}/logs/${shortdate}.log">
        <layout xsi:type="JsonLayout" includeAllProperties="true">
          <attribute name="time" layout="${longdate}" />
          <attribute name="level" layout="${level:upperCase=true}"/>
          <attribute name="message" layout="${message}" />
          <attribute name="Errormessage" layout="${exception:format=ToString,StackTrace}${newline}" />
        </layout>
      </target>
    </targets>
    <targets>
      <target xsi:type="File" name="jsonFileES" fileName="${basedir}/logs/${shortdate}-ES.log">
        <layout xsi:type="JsonLayout" includeAllProperties="true">
          <attribute name="time" layout="${longdate}" />
          <attribute name="level" layout="${level:upperCase=true}"/>
          <attribute name="balno" layout="${message}"/>
          <attribute name="message" layout="${exception:format=ToString,StackTrace}${newline}" />
        </layout>
      </target>
    </targets>
    <rules>
      <logger name="ES" levels="Info,Error" writeTo="jsonFileES" />
      <logger name="App" levels="Info,Error" writeTo="jsonFile" />
    </rules>
  </nlog>

  <appSettings file="AppSettingSecret.config">
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
